#include "main.h"
#include <stdlib.h>
#include <string.h>
char *string_nconcat(char *s1, char *s2, unsigned int n)
{
	char *concat;
	int i, length;

	length = strlen(s1) + strlen(memset(s1, n));
	concat = malloc(length);
	if (concat == NULL)
		return (NULL);
	for (i = 0; i < strlen(s1); i++)
	{
		concat[i] = s1[i];
	}
	for (j = 0; j < strlen(memset(s1, n)); j++)
	{
		concat[i + j] = s2[j];
	}
	return (concat)
