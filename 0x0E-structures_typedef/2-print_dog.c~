#include "dog.h"
#include <stdio.h>
void print_dog(struct dog *d)
{
	if (d.name == NULL)
		printf("Name: (nil)\n");
	printf("Name: %s\n", d.name);
	if (d.age == NULL)
		printf("Age: (nil)\n");
	printf("Name: %d\n", d.age);
	if (d.owner == NULL)
		printf("Owner: (nil)\n");
	printf("Owner: %s\n", d.owner);
}
